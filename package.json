{
  "name": "superforms",
  "version": "2.0.0",
  "description": "Just superforms",
  "main": "index.js",
  "scripts": {
    "createdb": "chmod ugo+x ./src/server/db/createdb.sh && ./src/server/db/createdb.sh",
    "initdb": "babel-node --presets @babel/env ./test/server/data/init.js",
    "build:server": "webpack --config webpack/webpack.server.js",
    "build:server:watch": "webpack --config webpack/webpack.server.js --watch",
    "build:client:prod": "cross-env NODE_ENV=production webpack --config webpack/webpack.client.js",
    "build:client:dev": "cross-env NODE_ENV=development webpack --config webpack/webpack.client.js",
    "server:dev": "node -r dotenv/config ./dist/server.js dotenv_config_path=./env/dev | npx bunyan",
    "server:prod": "node -r dotenv/config ./dist/server.js dotenv_config_path=./env/prod | npx bunyan",
    "start:dev": "npm run build:server && npm run server:dev",
    "start:prod": "(npm run build:server & npm run build:client:prod) && npm run server:prod",
    "prod": "env $(grep -v '^#' ./env/prod | xargs) forever start dist/server.js",
    "dev": "nodemon --exec \"node -r dotenv/config\" dist/server.js dotenv_config_path=./env/dev | npx bunyan",
    "debug": "nodemon --exec \"node -r dotenv/config\" dist/server.js dotenv_config_path=./env/debug | npx bunyan",
    "test": "npx mocha-webpack --opts ./test/mocha-webpack.opts"
  },
  "keywords": [
    "superforms"
  ],
  "author": "",
  "license": "MIT",
